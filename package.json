{
  "name": "ippo",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "build": "npx tsc -p tsconfig.build.json && tsc-alias",
    "start": "node dist/server.js",
    "dev": "dotenv -e .env.dev -- nodemon",
    "reformat": "npx prettier . --write",
    "db-create": "docker compose up -d",
    "db-migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev",
    "db-migrate:prd": "npx prisma migrate dev",
    "db-seed": "dotenv -e .env.dev -- sh -c 'npx prisma db push --force-reset && npx prisma db seed'",
    "db-studio": "dotenv -e .env.dev -- npx prisma studio",
    "db-migrate-test": "dotenv -e .env.test -- npm run db-migrate",
    "db-seed-test": "dotenv -e .env.test -- sh -c 'npx prisma db push --force-reset && npx prisma db seed'",
    "db-studio-test": "dotenv -e .env.test -- npx prisma studio",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "dotenv -e .env.test -- vitest --config ./vitest.config.ts"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node src/prisma/seeding/seedAll.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "./tests",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "ext": "ts, js"
  },
  "keywords": [],
  "author": "FlamingoCorp",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6.4.14",
    "@types/pug": "^2.0.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^5.9.1",
    "@prisma/client": "^5.11.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.3",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.2.1",
    "stripe": "^14.23.0",
    "typedi": "^0.10.0"
  }
}
